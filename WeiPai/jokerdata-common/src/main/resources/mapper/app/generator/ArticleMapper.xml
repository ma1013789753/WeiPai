<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jokerdata.mapper.app.generator.ArticleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jokerdata.entity.app.generator.Article">
        <id column="article_id" property="articleId"/>
        <result column="cate_id" property="cateId"/>
        <result column="article_name" property="articleName"/>
        <result column="article_content" property="articleContent"/>
        <result column="article_code" property="articleCode"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        article_id, cate_id, article_name, article_content, article_code
    </sql>

</mapper>
