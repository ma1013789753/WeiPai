<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jokerdata.mapper.app.generator.ConfigMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jokerdata.entity.app.generator.Config">
        <id column="config_id" property="configId"/>
        <result column="config_name" property="configName"/>
        <result column="config_code" property="configCode"/>
        <result column="config_content" property="configContent"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        config_id, config_name, config_code, config_content
    </sql>

    <select id="selectPage" resultMap="BaseResultMap">

        select
        <include refid="Base_Column_List"/>
        from g_config
        where del = 0
        <if test=" param.search1 != ''">
            and g_config.name LIKE CONCAT('%',#{param.search1},'%')
        </if>

    </select>
</mapper>
