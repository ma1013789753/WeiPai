<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jokerdata.mapper.app.generator.AuthGroupAccessMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jokerdata.entity.app.generator.AuthGroupAccess">
        <result column="uid" property="uid"/>
        <result column="group_id" property="groupId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uid, group_id
    </sql>

    <select id="selectPage" resultMap="BaseResultMap">

        select
        <include refid="Base_Column_List"/>
        from g_auth_group_access
        where del = 0
        <if test=" param.search1 != ''">
            and g_auth_group_access.name LIKE CONCAT('%',#{param.search1},'%')
        </if>

    </select>
</mapper>
